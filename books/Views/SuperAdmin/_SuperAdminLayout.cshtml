@{
    Layout = "_Layout";
}
@if (ViewBag?.Message != null)
{
    <div class="alert alert-success alert-dismissible fade show" role="alert">
        <i class="bi bi-bookmark-check-fill"></i>
        @ViewBag.Message
        <button type="button"
                class="btn-close"
                data-bs-dismiss="alert"
                aria-label="Close"></button>
    </div>
}
<nav class="navbar navbar-expand-lg bg-light mb-3">
<div class="container-fluid">
<a asp-action="Index"
   asp-controller="SuperAdmin"
   class="navbar-brand">Super Admin Panel</a>
<button class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarNavDropdown"
        aria-controls="navbarNavDropdown"
        aria-expanded="false"
        aria-label="nav switcher">
    <span class="navbar-toggler-icon"></span>
</button><div class="collapse navbar-collapse" id="navbarNavDropdown">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a asp-action="ManageUsersClaims"
                   id="superAdminMenuId2"
                   class="nav-link" >Manage Users Claims</a>
            </li>
        </ul>
    </div>
</div>
</nav>
